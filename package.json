{
  "name": "aws-cdk-services-playground",
  "version": "0.1.0",
  "bin": {
    "aws-cdk-services-playground": "bin/aws-cdk-services-playground.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write .",
    "cdk:up": "npm run format && npm run build && cdk deploy --profile greenline-sandbox-it-1 --all",
    "cdk:down": "cdk destroy --profile greenline-sandbox-it-1 --all"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.15",
    "aws-cdk": "2.56.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.56.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
